
<meta charset="UTF-8">
<meta http-equiv="X-UA-Compatible" content="IE=edge" />
<meta name="viewport" content="width=device-width, initial-scale=1" />

<title>
  {%- if title -%}
    {{title}} | {{site.title}}
  {%- else -%}
    {{site.title}}
  {%- endif -%}
</title>
<meta name="description" content=" {% getDesc page, description, site.description %}"/>
<link rel="canonical" href="{{- site.url -}} {{- page.url }}">
{% if blockRobots -%} <meta name="robots" content="noindex, follow"> {%- endif %}

{#- -#}
{%- include "_head/assets.njk" -%}
{%- include "_head/favicon.njk" -%}
{# TODO: social #}
{%- include "_head/social.njk" -%}
